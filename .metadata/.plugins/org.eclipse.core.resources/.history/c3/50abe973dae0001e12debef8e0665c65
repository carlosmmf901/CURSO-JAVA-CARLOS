package exercicios;

public class ThreadLab extends Thread {
	public ThreadLab(String mensagem) {
		super(mensagem);
		
	}
	
	@Override
	public void run() {
		 for (int i = 0; i < 10; i++) {
			 System.out.println(getName());
			 try {
				 sleep(500);
			 } catch(InterruptedException e) {
				 System.out.println(e);
			 }
			 
			 
		 }
	}
	
	
}
